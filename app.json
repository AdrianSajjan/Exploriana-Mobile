{
  "expo": {
    "name": "Exploriana",
    "slug": "exploriana",
    "version": "1.0.0",
    "icon": "./assets/images/app-icon.png",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/images/splashscreen.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9c112cdc-56f9-441b-be14-aeee3344f6e6"
      }
    },
    "android": {
      "package": "com.exploriana",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC4-GJwtpUaMp0u2sp75yzaYwLPOmYIplU"
        }
      }
    }
  }
}
